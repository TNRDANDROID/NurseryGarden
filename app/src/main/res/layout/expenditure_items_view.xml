                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:padding="10dp"
                                            android:text="@string/semi_colan"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/content_text_size" />

                                    </LinearLayout>


                                    <LinearLayout
